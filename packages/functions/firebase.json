{
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "functions"
    ],
    "public": ".",
    "rewrites": []
  },
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "emulators": {
    "hosting": {
      "port": 3000
    },
    "ui": {
      "enabled": true,
      "port": 3001
    },
    "functions": {
      "port": 3002
    },
    "firestore": {
      "port": 3003
    }
  }
}